{
   "extends": "@strapi/typescript-utils/tsconfigs/server",
   "compilerOptions": {
      "outDir": "dist",
      "rootDir": ".",
      "jsx": "react-jsx",
      "lib": ["ES2020", "DOM"]
   },
   "include": ["./", "src/**/*.json"],
   "exclude": [
      "node_modules/",
      "build/",
      "dist/",
      ".cache/",
      ".tmp/",
      "src/admin/",
      "**/*.test.ts",
      "src/plugins/**"
   ]
}
